{
  "name": "reshaped",
  "private": true,
  "version": "2.0.0-nightly.5",
  "author": "Dmitry Belyaev <blv.dmitry@gmail.com>",
  "main": "./index.js",
  "jsnext:main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "components/**/*",
    "constants/**/*",
    "hooks/**/*",
    "icons/**/*",
    "styles/**/*",
    "themes/**/*",
    "types/**/*",
    "utilities/**/*",
    "config/**/*",
    "cli/**/*",
    "bin/**/*",
    "index.js",
    "index.d.ts",
    "bundle.js",
    "bundle.js.map",
    "bundle.css",
    "bundle.css.map",
    "bundle.d.ts"
  ],
  "bin": {
    "reshaped": "./bin/cli.js"
  },
  "scripts": {
    "clean": "rm -rf dist components constants hooks icons styles themes types cli utilities && rm -f index.js index.d.ts",
    "build": "yarn clean && yarn build:stories && yarn build:esm && yarn build:cjs && yarn build:css && yarn build:bundle && resolve-tspaths && yarn release:before",
    "build:themes": "yarn build:cjs && node bin/cli.js theming --config cli/theming/reshaped.config.js --output src/themes --private",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:css": "postcss \"src/**/*.css\" --dir dist --base src --config tools/build && cp -r src/themes/media.css dist/themes/media.css && mkdir themes && cp -r src/themes/media.css themes/media.css",
    "build:stories": "tsc -p tsconfig.stories.json",
    "build:bundle": "webpack --config tools/webpack/webpack.config.js && cp dist/index.d.ts dist/bundle.d.ts",
    "start": "storybook dev -c tools/storybook -p 3000",
    "build:storybook": "storybook build -c tools/storybook -o dist/app --disable-telemetry",
    "build:chromatic": "STORYBOOK_ENV=chromatic storybook build -c tools/storybook",
    "release:before": "cp -r dist/* ./",
    "release": "yarn version && yarn pack && mkdir ../../releases/$(jq -r .version package.json) && mv *.tgz ../../releases/$(jq -r .version package.json)/reshaped-react-v$(jq -r .version package.json).tgz",
    "release:test": "yarn pack --filename reshaped-beta.tgz",
    "chromatic": "chromatic -b build:chromatic --project-token=jbcohq2nbtr",
    "test:vrt": "yarn chromatic",
    "test:unit": "jest --config tools/jest/jest.config.js"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "browserslist": [
    "defaults and not IE 11"
  ],
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-typescript": "7.21.5",
    "@storybook/addon-a11y": "7.0.18",
    "@storybook/addon-styling": "1.0.8",
    "@storybook/react": "7.0.18",
    "@storybook/react-vite": "7.0.18",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.2",
    "babel-jest": "29.5.0",
    "babel-loader": "9.1.2",
    "chromatic": "6.17.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-matchmedia-mock": "1.1.0",
    "next-mdx-remote": "4.4.1",
    "postcss": "8.4.24",
    "postcss-cli": "10.1.0",
    "postcss-each": "1.1.0",
    "postcss-import": "15.1.0",
    "postcss-loader": "7.3.0",
    "postcss-nested": "6.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.0.18",
    "ts-jest": "29.1.0",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0"
  },
  "peerDependencies": {
    "postcss": "^8.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "autoprefixer": "10.4.14",
    "chalk": "4.1.2",
    "commander": "10.0.1",
    "cssnano": "6.0.1",
    "postcss-custom-media": "8.0.2"
  },
  "gitHead": "0e94341f99572eacf1e476054b080bce2c902cea"
}
